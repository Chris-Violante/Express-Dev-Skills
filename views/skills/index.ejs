<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Develeper Skills Lab</title>
</head>
<body>
    <h1>Developer Skills</h1>
    <ul>
      <% skills.forEach(function(s) { %>
        <li>
          <a href="/skills/<%= s.id %>">  <%= s.skill %></a>
            - 
          <%= s.learned ? 'Yes' : 'Not Yet' %>
        </li>
      <% }); %>
    </ul>
  </body>
</html>